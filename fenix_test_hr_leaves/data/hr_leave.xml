<odoo>
    <data noupdate="1">

        <record id="hr_holidays_honeymoon" model="hr.leave">
            <field name="name">Trip for Honeymoon</field>
            <field name="holiday_status_id" ref="hr_holidays.holiday_status_sl"/>
            <field eval="time.strftime('%Y-%m-01')" name="date_from"/>
            <field eval="time.strftime('%Y-%m-02')" name="date_to"/>
            <field name="number_of_days">1</field>
            <field name="employee_id" ref="fenix_test_hr.employee_ronnie"/>
        </record>

        <record id="hr_holidays_wedding" model="hr.leave">
            <field name="name">Wedding Anniversary</field>
            <field name="holiday_status_id" ref="hr_holidays.holiday_status_sl"/>
            <field eval="time.strftime('%Y-%m-20')" name="date_from"/>
            <field eval="time.strftime('%Y-%m-21')" name="date_to"/>
            <field name="number_of_days">1</field>
            <field name="employee_id" ref="fenix_test_hr.employee_anita"/>
            <field name="state">confirm</field>
        </record>
    </data>
</odoo>
